} </h1>\r\n      <ItemList productos={itemProducto} />\r\n    </>\r\n  )\r\n}\r\n\r\nexport default ItemLIstContainer\r\n","C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\components\\items\\ItemList.js",[],"C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\components\\items\\Item.js",["72"],"\r\nimport React, { useContext, useState } from 'react'\r\nimport { Link } from 'react-router-dom';\r\nimport { CartContext } from '../../context/CartContext';\r\nimport ItemCount from '../contador/ItemCount';\r\nimport './item.css';\r\n\r\nconst Item = ({ producto }) => {\r\n\r\n  const [carrito, setCarrito] = useState(false)\r\n  const [contador, setContador] = useState(1);\r\n\r\n  const { AddItem } = useContext(CartContext)\r\n\r\n  const onAdd = () => {\r\n    AddItem({ id: (producto.id), item: (producto), quantity: (contador) });\r\n    setCarrito(true);\r\n  };\r\n\r\n  const agregar = () => {\r\n    if (contador < producto.stock) {\r\n      setContador(contador + 1);\r\n    } else {\r\n      alert('no se pueden agregar mas productos')\r\n    }\r\n  }\r\n\r\n  const restar = () => {\r\n    if (contador > 1) {\r\n      setContador(contador - 1);\r\n    }\r\n  }\r\n\r\n\r\n  return (\r\n    <>\r\n      <div className='container row-cols-lg-10'>\r\n        <div className='card card-list'>\r\n          <Link to={`/detalle/${producto.id}`}><div className='imgProduct'><img src={producto.image} /></div></Link>\r\n          <p>{producto.title}</p>\r\n          <p>${producto.price}</p>\r\n          <p>stock:{producto.stock}unds</p>\r\n        </div>\r\n      </div>\r\n      <div>\r\n        {carrito ? <Link to={`/cart`}><button> Ir al carrito </button></Link> :\r\n          <ItemCount restar={restar} agregar={agregar} contador={contador} onAdd={onAdd} />\r\n        }\r\n      </div>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default Item;\r\n","C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\containers\\ItemDatailContainer.js",["73"],"\r\nimport React, { useState, useEffect, useContext } from 'react'\r\nimport { Link, useParams } from 'react-router-dom';\r\nimport ItemCount from '../components/contador/ItemCount';\r\nimport ItemDetail from '../components/items/ItemDetail';\r\nimport { getFirestore } from '../firebase';\r\nimport { CartContext } from '../context/CartContext';\r\nimport spinner from '../spinner.png';\r\nimport './container.css';\r\n\r\nconst ItemDatailContainer = () => {\r\n\r\n  const [contador, setContador] = useState(1);\r\n  const [detalle, setDetalle] = useState([]);\r\n  const [loading, setLoading] = useState(false);\r\n  const [carrito, setCarrito] = useState(false)\r\n\r\n  const agregar = () => {\r\n    if (contador < detalle.stock) {\r\n      setContador(contador + 1);\r\n    } else {\r\n      alert('no se pueden agregar mas productos')\r\n    }\r\n  }\r\n\r\n  const restar = () => {\r\n    if (contador > 1) {\r\n      setContador(contador - 1);\r\n    }\r\n  }\r\n\r\n  const onAdd = () => {\r\n    AddItem({ id: (id), item: (detalle), quantity: (contador) });\r\n    setCarrito(true);\r\n  };\r\n\r\n  const { AddItem } = useContext(CartContext);\r\n\r\n  const { id } = useParams();\r\n\r\n  useEffect(() => {\r\n    setLoading(true);\r\n\r\n    const db = getFirestore()\r\n    const itemCollection = db.collection('PRODUCTOS')\r\n    const item = itemCollection.doc(id)\r\n    item.get().then((value) => {\r\n      setDetalle(value.data())\r\n      setLoading(false)\r\n    });\r\n  }, [id, setDetalle]);\r\n\r\n  if (loading) {\r\n    return <div className='spinner'><img src={spinner} /></div>\r\n  }\r\n\r\n  return (\r\n    <>\r\n      {detalle !== undefined ?\r\n        <div className='card'>\r\n          <ItemDetail detalle={detalle} />\r\n          {carrito ? <Link to={`/cart`}><button>ir al carrito</button></Link>\r\n            : <ItemCount restar={restar} agregar={agregar} contador={contador} onAdd={onAdd} />\r\n          }\r\n        </div> :\r\n        <h1>NO SE ENCONTRO PRODUCTO EN LISTA</h1>\r\n      }\r\n    </>\r\n  )\r\n\r\n\r\n}\r\n\r\nexport default ItemDatailContainer;\r\n","C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\components\\items\\ItemDetail.js",["74"],"\r\nimport React from 'react';\r\nimport './item.css';\r\n\r\nconst ItemDetail = ({ detalle }) => {\r\n  return (\r\n    <>\r\n      <div className='container container-mark'>\r\n        <div className='card'>\r\n          <h2>{detalle.name}</h2>\r\n          <div className='row justify-content-center cols-lg-12 card-body'>\r\n            <img src={detalle.image} />\r\n            <h3>${detalle.price}</h3>\r\n            <p>{detalle.description}</p>\r\n            <p>stock:{detalle.stock}unds.</p>\r\n            <p>Envios a todo el pais.</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </>\r\n  )\r\n\r\n}\r\n\r\nexport default ItemDetail;\r\n","C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\containers\\Categorias.js",["75"],"\r\nimport React, { useState, useEffect } from 'react';\r\nimport { useParams } from 'react-router-dom';\r\nimport ItemList from '../components/items/ItemList';\r\nimport { getFirestore } from '../firebase';\r\nimport spinner from '../spinner.png';\r\nimport './container.css';\r\n\r\n\r\nconst Categorias = () => {\r\n\r\n  const [categoryProduc, setCategoryProduc] = useState([]);\r\n  const [loading, setLoading] = useState(false);\r\n\r\n  const { idTipo } = useParams();\r\n  console.log(idTipo);\r\n\r\n  useEffect(() => {\r\n    setLoading(true);\r\n    const db = getFirestore()\r\n    const itemCollection = db.collection('PRODUCTOS')\r\n\r\n    const category = itemCollection.where('category', '==', idTipo)\r\n\r\n    category.get().then((value) => {\r\n      setCategoryProduc(value.docs.map(e => (e.data())))\r\n      setLoading(false)\r\n    })\r\n\r\n  }, [idTipo, setCategoryProduc]);\r\n\r\n  if (loading) {\r\n    return <div className='spinner'><img src={spinner} /></div>\r\n  }\r\n\r\n\r\n  return (\r\n    <>\r\n      <ItemList productos={categoryProduc} />\r\n    </>\r\n  )\r\n}\r\n\r\nexport default Categorias;\r\n\r\n","C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\components\\Navbar\\Navbar.js",[],"C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\context\\CartContext.js",[],"C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\components\\Navbar\\CartWidget.js",[],"C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\firebase\\index.js",[],"C:\\Users\\MEME Y DAVID\\Desktop\\proyectocursoreact\\src\\components\\cart\\Cart.js",["76"],"import React, { useContext, useRef, useState } from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport { CartContext } from '../../context/CartContext';\r\nimport { getFirestore } from '../../firebase';\r\nimport { Modal, ModalBody, ModalFooter, ModalHeader, Button, Form, FormGroup, FormText } from 'reactstrap'\r\nimport { FormControl, FormLabel } from 'react-bootstrap';\r\n\r\nconst Cart = () => {\r\n\r\n  const { cart, RemoveItem, ClearCart, priceTotal } = useContext(CartContext);\r\n\r\n  const [modal, setModal] = useState(false)\r\n\r\n  const formName = useRef('')\r\n  const formPhono = useRef('')\r\n  const formEmail = useRef('')\r\n  const formComent = useRef('')\r\n\r\n  const FinDeCompra = () => {\r\n    let orders = ({ usuario: { name: formName.current.value, email: formEmail.current.value, tel: formPhono.current.value, comentario: formComent.current.value }, item: [...cart], total: priceTotal })\r\n\r\n    const db = getFirestore()\r\n    const InsertOrders = db.collection('ordenes')\r\n    InsertOrders.add(orders).then((value) => {\r\n      alert('El Numero de orden de su pedido es: ' + (value.id))\r\n    })\r\n\r\n    setModal(false)\r\n    ClearCart()\r\n  }\r\n\r\n  return (\r\n    <>\r\n      {cart.length === 0 ?\r\n        <div>\r\n          <h1>Carrito vacio</h1>\r\n          <Link to={`/`}><button>Continua tu compra</button></Link>\r\n        </div> :\r\n        <div>\r\n          <h1>Este carrito tiene</h1>\r\n          <button onClick={() => ClearCart()}>Vaciar carrito</button>\r\n          <br />\r\n          <h2>El precio total de los productos es: $ {priceTotal}</h2>\r\n          <ul>\r\n            {cart.map(elemento => {\r\n              return (\r\n                <li key={elemento.id}>\r\n                  <img src={elemento.item.image} />\r\n                  <h3>{elemento.item.name}</h3>\r\n                  <h3>cantidad : {elemento.quantity} </h3>\r\n                  <h3>valor:$ {elemento.item.price}</h3>\r\n                  <h4>Total:$ {elemento.quantity * elemento.item.price}</h4>\r\n                  <button onClick={() => RemoveItem(elemento.id)} >Eliminar producto de lista</button>\r\n                  <br />\r\n                </li>\r\n              )\r\n            })\r\n            }\r\n          </ul>\r\n          <div>\r\n            {cart.length > 0 ? <button onClick={() => { setModal(true) }}>Finalizar Checkout </button> :\r\n              null}\r\n          </div>\r\n        </div>\r\n      }\r\n      <>\r\n        <Modal isOpen={modal}>\r\n          <ModalHeader closeButton>\r\n            <h2>Complete los campos para realizar su pedido</h2>\r\n          </ModalHeader>\r\n          <ModalBody>\r\n            <Form>\r\n              <FormGroup>\r\n                <FormLabel>Nombre</FormLabel>\r\n                <FormControl type='text' ref={formName} placeholder='Nombre y Apellido'></FormControl>\r\n              </FormGroup>\r\n              <FormGroup>\r\n                <FormLabel>Telefono</FormLabel>\r\n                <FormControl type='text' ref={formPhono}></FormControl>\r\n              </FormGroup>\r\n              <FormGroup controlId=\"formBasicEmail\">\r\n                <FormLabel>Email</FormLabel>\r\n                <FormControl type=\"email\" ref={formEmail} placeholder=\"xxxxx@xxxx.xxx\"></FormControl>\r\n                <FormControl type=\"email\" placeholder=\"xxxxx@xxxx.xxx\"></FormControl>\r\n                <FormText className='text-muted'>Por favor introduzca nuevamente su Email</FormText>\r\n              </FormGroup>\r\n              <FormGroup controlId=\"formBasicPassword\">\r\n                <FormLabel>Comentarios</FormLabel>\r\n                <FormControl as='textarea' rows='{3}' ref={formComent} placeholder=\"Comentario extra\" />\r\n              </FormGroup>\r\n            </Form>\r\n          </ModalBody>\r\n          <ModalFooter>\r\n            <Button variant=\"secondary\" onClick={() => { setModal(false) }}>Cerrar ventana</Button>\r\n            <Link to={`/`}><Button variant=\"primary\" type=\"submit\" onClick={() => { FinDeCompra() }}>Finalizar compra</Button></Link>\r\n          </ModalFooter>\r\n        </Modal>\r\n      </>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default Cart;\r\n",{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":29,"column":37,"nodeType":"83","endLine":29,"endColumn":58},{"ruleId":"81","severity":1,"message":"82","line":39,"column":76,"nodeType":"83","endLine":39,"endColumn":104},{"ruleId":"81","severity":1,"message":"82","line":54,"column":37,"nodeType":"83","endLine":54,"endColumn":58},{"ruleId":"81","severity":1,"message":"82","line":12,"column":13,"nodeType":"83","endLine":12,"endColumn":40},{"ruleId":"81","severity":1,"message":"82","line":33,"column":37,"nodeType":"83","endLine":33,"endColumn":58},{"ruleId":"81","severity":1,"message":"82","line":48,"column":19,"nodeType":"83","endLine":48,"endColumn":52},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]